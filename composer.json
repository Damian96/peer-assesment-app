{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "biscolab/laravel-recaptcha": "^4.0",
    "davejamesmiller/laravel-breadcrumbs": "5.3.0",
    "egulias/email-validator": "^2.1",
    "fideloper/proxy": "^4.0",
    "itsgoingd/clockwork": "^4.0",
    "laravel/framework": "^6.0",
    "laravel/tinker": "^1.0",
    "spatie/laravel-html": "^2.24"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.6",
    "facade/ignition": "^1.4",
    "fzaninotto/faker": "^1.4",
    "krlove/eloquent-model-generator": "^1.3",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^3.0",
    "phpunit/phpunit": "^8.0"
  },
  "provide": {
    "phpunit/phpunit": "8.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "platform": {
      "php": "7.3.11"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "classmap": [
      "database/seeds",
      "database/factories"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": [
      "vendor/bin/phpunit -c phpunit.xmlAPP_NAME=OPESAdds testing directory"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "cache-clear": [
      "@php artisan optimize:clear",
      "@php artisan cache:clear",
      "@php artisan view:clear",
      "@php artisan event:clear",
      "@php artisan config:clear",
      "@php artisan route:clear",
      "@php artisan clockwork:clean",
      "@php artisan auth:clear-resets"
    ],
    "cache-store": [
      "@php artisan optimize",
      "@php artisan view:cache",
      "@php artisan event:cache",
      "@php artisan config:cache",
      "@php artisan route:cache"
    ],
    "laravel-init": [
      "./xampp.sh",
      "composer cache-clear"
    ],
    "pre-commit": [
      "@php artisan ide-helper:models",
      "composer cache-store"
    ]
  }
}
